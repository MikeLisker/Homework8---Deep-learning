        "# --- PASO 4: ENTRENAMIENTO ---\n",
        "device = torch.device(\"cuda\" if torch.cuda.is_available() else \"mps\" if torch.backends.mps.is_available() else \"cpu\")\n",
        "print(f\"\\nUsando dispositivo: {device}\")\n",
